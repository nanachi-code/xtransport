extends ../layer/home

prepend title-element
	- var elementTitle = 'Widget Default'

prepend body
	- var ClassBody = 'kopa-sub-page woocommerce woocommerce-page'

append css
	// woocommerce style
	link(rel="stylesheet", type="text/css", href="css/woocommerce-layout.min.css")
	link(rel="stylesheet", type="text/css", href="css/woocommerce.min.css")

block head-base
	include ../include/head

block header-area
	include ../main/header-01

block area-01
	section.kopa__area.kopa__area--noSpace
		.container
			include ../widget/top-page
		// container
	// kopa-area

block area-02
	section.kopa__area.kopa__area--08
		.container
			.widget
				.widget-title.style--08
					h2.primary__title #{elementTitle}

					.kopa__line--dotted.style--02
						span.kopa__line--dotted--small.kopa__line--dotted--dark
						span.kopa__line--dotted--small.kopa__line--dotted--gray
						span.kopa__line--dotted--small.kopa__line--dotted--light
						span.kopa__line--dotted--wide
					// kopa line dotted

					p.text__title Transcargo makes business flow. As one of the world’s leading non-asset-based supply chain management companies, we design and implemenindustry-leading solutions in both freight management.Over 42,000 dedicated employees, working in 17 regional clusters around the globe, deliver operational excellence — to provide viable answers to the most challenging supply chain questions. trucking companies as well as Rail Road expediters allow us to offer a wide spectrum of services in a one-stop solution for all transportation needs in a supply chain process while simultaneously preserving the efficiency of specialty units.
				// widget title
			// widget

			.row.row-custom
				.col-xs-12.col-md-3
					include ../widget/default-archive-01
				// col

				.col-xs-12.col-md-3
					include ../widget/default-archive-02
				// col

				.col-xs-12.col-md-3
					include ../widget/default-calendar
				// col

				.col-xs-12.col-md-3
					include ../widget/default-category-01
				// col
			// row

			.row.row-custom
				.col-xs-12.col-md-3
					include ../widget/default-category-02
				// col

				.col-xs-12.col-md-3
					include ../widget/default-meta
				// col

				.col-xs-12.col-md-3
					include ../widget/default-nav-menu
				// col

				.col-xs-12.col-md-3
					include ../widget/default-page
				// col
			// row

			.row.row-custom
				.col-xs-12.col-md-3
					include ../widget/default-recent-comment
				// col

				.col-xs-12.col-md-3
					include ../widget/default-recent-entry
				// col

				.col-xs-12.col-md-3
					include ../widget/default-rss
				// col

				.col-xs-12.col-md-3
					include ../widget/default-search
				// col
			// row

			.row.row-custom
				.col-xs-12.col-md-6
					include ../widget/default-tag
				// col

				.col-xs-12.col-md-6
					include ../widget/default-text
				// col
			// row

			.row.row-custom
				.col-xs-12.col-md-3
					include ../widget/woo-product
				// col

				.col-xs-12.col-md-3
					include ../widget/woo-product-category
				// col

				.col-xs-12.col-md-3
					include ../widget/woo-product-category-2
				// col

				.col-xs-12.col-md-3
					include ../widget/woo-product-search
				// col
			// row

			.row.row-custom
				.col-xs-12.col-md-3
					include ../widget/woo-rate-product
				// col

				.col-xs-12.col-md-3
					include ../widget/woo-recent-review
				// col

				.col-xs-12.col-md-3
					include ../widget/woo-shopping-cart
				// col

				.col-xs-12.col-md-3
					include ../widget/woo-tag
				// col
			// row
		// container
	// kopa-area

block area-03
	section.kopa__area.kopa__area--09
		.container
			include ../widget/social-02
		// container
	// kopa-area

block bottom-sidebar
	include ../main/bottom-sidebar-01

block footer-area
	include ../main/footer

block js-base
	include ../include/js